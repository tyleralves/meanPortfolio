<div project-list-current current-project="$ctrl.currentProject" is-medium-device="$ctrl.isMediumDevice" class="row pl-image-row">
    <div class="col-xs-1 pull-right visible-xs-block visible-sm-block pl-indicators-col">
        <div class="pl-indicators-container">
            <ol class="carousel-indicators">
                <li ng-click="$ctrl.changeCurrentProject(1)" ng-class="$ctrl.indicatorIsActive(1)"></li>
                <li ng-click="$ctrl.changeCurrentProject(2)" ng-class="$ctrl.indicatorIsActive(2)"></li>
                <li ng-click="$ctrl.changeCurrentProject(3)" ng-class="$ctrl.indicatorIsActive(3)"></li>
                <li ng-click="$ctrl.changeCurrentProject(4)" ng-class="$ctrl.indicatorIsActive(4)"></li>
                <div class="glyphicon glyphicon-question-sign pl-help-btn" ng-click="$ctrl.showHelp = !$ctrl.showHelp"></div>
            </ol>
        </div>

    </div>


    <div class="col-xs-11 col-md-12">
        <div class="row">
            <div class="col-md-4 pl-image-col">
                <div ng-swipe-right="$ctrl.slideOutClass = true" ng-swipe-left="$ctrl.slideOutClass = false" project-image-highlight project-num="1" active-highlight-project="$ctrl.activeHighlightProject" ng-show="$ctrl.isCurrentProject(1)" class="pl-image-outer-container">
                    <div class="pl-image-inner-container pl-image-1">
                        <div class="pl-image-scroll">
                            <div class="pl-image-help" ng-show="$ctrl.showHelp">
                                <div class="glyphicon glyphicon-remove-circle pl-image-help-close" ng-click="$ctrl.showHelp = !$ctrl.showHelp"></div>
                                <img class="pl-image-help-icon" src="/public/images/swipe.png">
                            </div>
                            <div class="pl-image-caption" ng-class="$ctrl.slideOutClass?'pl-image-caption-swiped':''" >
                                <div class="pl-desc-container">
                                    <h4 class="pl-desc-title">
                                        Notable Labs Dashboard
                                    </h4>
                                    <div class="pl-desc-summary">
                                        An MVP dashboard that allows Notable Labs staff to manage and assign staff to patients, update patient status, and submit test results. Physician clients are able to submit new patients, view their patient's status and test results, and communicate with the staff member assigned to their patients. The app is written in Ruby on Rails using a PostgreSQL database and an Angular 1.5 front-end.
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <a href="https://notable-sample.herokuapp.com/" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Deployed App</div>
                                            </a>
                                        </div>
                                        <div class="col-md-4">
                                            <a href="https://vimeo.com/182626441" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Demo Video</div>
                                            </a>
                                        </div>
                                        <div class="col-md-4">
                                            <a href="https://github.com/tyleralves/notable-sample" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Github Repo</div>
                                            </a>
                                        </div>
                                    </div>

                                    <div class="pl-desc-labels">
                                        <span class="label pl-desc-label">Ruby on Rails</span>
                                        <span class="label pl-desc-label">Angular 1.5</span>
                                        <span class="label pl-desc-label">PostgreSQL</span>
                                        <span class="label pl-desc-label">Devise Auth</span>
                                        <span class="label pl-desc-label">Heroku</span>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-4 pl-image-col">
                <div ng-swipe-right="$ctrl.slideOutClass = !$ctrl.slideOutClass" ng-swipe-left="$ctrl.slideOutClass = false" project-image-highlight project-num="2" active-highlight-project="$ctrl.activeHighlightProject" ng-show="$ctrl.isCurrentProject(2)" class="pl-image-outer-container pl-image-container-2">
                    <div class="pl-image-inner-container pl-image-2">
                        <div class="pl-image-scroll">
                            <div class="pl-image-help" ng-show="$ctrl.showHelp">
                                <div class="glyphicon glyphicon-remove-circle pl-image-help-close" ng-click="$ctrl.showHelp = !$ctrl.showHelp"></div>
                                <img class="pl-image-help-icon" src="/public/images/swipe.png">
                            </div>
                            <div class="pl-image-caption" ng-class="$ctrl.slideOutClass?'pl-image-caption-swiped':''">
                                <div class="pl-desc-container" id="pl-desc-container-2">
                                    <h4 class="pl-desc-title">
                                        Volunteer Connect
                                    </h4>
                                    <div class="pl-desc-summary">
                                        Volunteer Connect cuts out the pricey middlemen and connects willing volunteers with the organizations that need help. I created linked Mongo collections (users, messages, feedback), developed backend Passport user authentication and used Handlebars on the front-end.
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <a href="http://stormy-eyrie-32571.herokuapp.com" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Deployed App</div>
                                            </a>
                                        </div>
                                        <div class="col-md-6">
                                            <a href="https://github.com/tyleralves/volunteerApp" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Github Repo</div>
                                            </a>
                                        </div>
                                    </div>
                                    <div class="pl-desc-labels">
                                        <span class="label pl-desc-label">NodeJS</span>
                                        <span class="label pl-desc-label">JQuery</span>
                                        <span class="label pl-desc-label">MongoDB</span>
                                        <span class="label pl-desc-label">Mongoose</span>
                                        <br>
                                        <span class="label pl-desc-label">Express</span>
                                        <span class="label pl-desc-label">Passport</span>
                                        <span class="label pl-desc-label">Handlebars</span>
                                        <span class="label pl-desc-label">Heroku</span>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div ng-swipe-right="$ctrl.slideOutClass = !$ctrl.slideOutClass" ng-swipe-left="$ctrl.slideOutClass = false" project-image-highlight project-num="3" active-highlight-project="$ctrl.activeHighlightProject" ng-show="$ctrl.isCurrentProject(3)" class="pl-image-outer-container pl-image-container-3">
                    <div class="pl-image-inner-container pl-image-3">
                        <div class="pl-image-scroll">
                            <div class="pl-image-help" ng-show="$ctrl.showHelp">
                                <div class="glyphicon glyphicon-remove-circle pl-image-help-close" ng-click="$ctrl.showHelp = !$ctrl.showHelp"></div>
                                <img class="pl-image-help-icon" src="/public/images/swipe.png">
                            </div>
                            <div class="pl-image-caption" ng-class="$ctrl.slideOutClass?'pl-image-caption-swiped':''">
                                <div class="pl-desc-container" id="pl-desc-container-3">
                                    <h4 class="pl-desc-title">
                                        Wild Trails - GovHack 2016
                                    </h4>
                                    <div class="pl-desc-summary">
                                        I created Wild Trails in 48 hours as my team’s lead developer for New Zealand’s GovHack 2016. The SPA detects user location or allows location input using Google Places API, queries the New Zealand's Department of Conservation ArcGIS Server API, then displays nearby hikes using the Google Maps API. I structured the client side using Angular, used ArcGIS Javascript API and built the backend using ExpressJS/ NodeJS.
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <a href="https://evening-shore-68966.herokuapp.com/" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Deployed App</div>
                                            </a>
                                        </div>
                                        <div class="col-md-4">
                                            <a href="https://vimeo.com/176897645" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Promo Video</div>
                                            </a>
                                        </div>
                                        <div class="col-md-4">
                                            <a href="https://github.com/tyleralves/wildtrails" class="pl-desc-link" target="_blank">
                                                <div class="btn pl-desc-btn">Github Repo</div>
                                            </a>
                                        </div>

                                    </div>
                                    <div class="pl-desc-labels">
                                        <span class="label pl-desc-label">NodeJS</span>
                                        <span class="label pl-desc-label">MongoDB</span>
                                        <span class="label pl-desc-label">Mongoose</span>
                                        <span class="label pl-desc-label">Express</span>
                                        <span class="label pl-desc-label">Angular 1.5</span>
                                        <span class="label pl-desc-label">Google Maps API</span>
                                        <span class="label pl-desc-label">ArcGIS API</span>
                                        <span class="label pl-desc-label">NZ DOC API</span>
                                        <span class="label pl-desc-label">Heroku</span>
                                    </div>
                                </div>
